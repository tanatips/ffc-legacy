                    <RadioButton
                        android:id="@+id/rdoStress9qQ4_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:text="@string/SomeDays_1_7"/>
                    <RadioButton
                        android:id="@+id/rdoStress9qQ4_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:text="@string/Frequent_7_days"/>
