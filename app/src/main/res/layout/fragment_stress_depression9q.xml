                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="20dp">
                    <RadioButton
                        android:id="@+id/rdoStress9qQ1_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:text="@string/NothingAtAll"/>
                    <RadioButton
                        android:id="@+id/rdoStress9qQ1_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:text="@string/SomeDays_1_7"/>
                    <RadioButton
                        android:id="@+id/rdoStress9qQ1_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:text="@string/Frequent_7_days"/>
